{
  "expo": {
    "name": "Krishi Sarthi",
    "slug": "krishisarthi",
    "description": "App for Farmers",
    "primaryColor": "#A0C49D",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#A0C49D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abhishek3459.krishisarthi"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#A0C49D"
      },
      "package": "com.abhishek3459.krishisarthi"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "d87c9e7d-8e8a-47bc-bea5-86a6b565d6b3"
      }
    }
  }
}
